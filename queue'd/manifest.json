{
  "manifest_version": 3,
  "name": "Queue'd",
  "version": "1.0",
  "description": "Queue manager for ChatGPT",
  "host_permissions": [
    "https://chat.openai.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}